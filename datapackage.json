{
  "name": "henry-gas-price",
  "title": "Henry Hub gas prices",
  "description": "List of the Henry Hub gas prices",
  "repository": {
    "type": "git",
    "url": "git://github.com/kittdotech/henry-gas-price.git"
  },
  "sources": [{
    "name": "EIA",
    "web": "http://www.eia.gov"
  }],
  "keywords": [
    "price",
    "henry"
  ],
  "resources": [
    {
      "name": "henry-gas-price-yearly",
      "path": "data/price_yearly.csv",
      "format": "csv",
      "mediatype": "text/csv",
      "schema": {
        "fields": [
          {
            "name": "Date",
            "description": "First Date of the year",
            "type": "date"
          },
          {
            "name": "price",
            "description": "Price in Dollars per Million Btu",
            "type": "float"
          }
        ]
      }
    },
    {
