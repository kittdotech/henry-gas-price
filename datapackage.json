      "format": "csv",
      "mediatype": "text/csv",
      "schema": {
        "fields": [
          {
            "name": "Date",
            "description": "First Date in the year",
            "type": "date"
          },
          {
            "name": "price",
            "description": "Price in Dollars per Million Btu",
            "type": "float"
          },
        ]
      },
    }
